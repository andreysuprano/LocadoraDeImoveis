<Window x:Class="LocadoraDeImoveis.Views.frmGerarPdf"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LocadoraDeImoveis.Views"
         xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"        
        mc:Ignorable="d"
        Title="Gerador de PDF" Background="#282A43"  WindowStartupLocation="CenterScreen" WindowStyle="SingleBorderWindow"  MaxHeight="300" MinHeight="300" MaxWidth="300" MinWidth="300">
    <Grid Background="#FFF" MaxHeight="300" MinHeight="300" MaxWidth="300" MinWidth="300">
        <TextBox x:Name="IdContrato" materialDesign:HintAssist.Hint="Id do Contrato" Margin="45,92,45,177"/>
        <Button x:Name="BrnGerarPdf" Style="{StaticResource MaterialDesignRaisedButton}" materialDesign:ButtonProgressAssist.Value="-1" materialDesign:ButtonProgressAssist.IsIndicatorVisible="True" materialDesign:ButtonProgressAssist.IsIndeterminate="True"
            Content="Gerar PDF" Margin="45,151,45,89" Height="60" Click="BrnGerarPdf_Click" />
    </Grid>
</Window>
